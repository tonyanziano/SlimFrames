<Ui xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
  <Script file="FigHud.lua"/>

  <Button name="FPlayerFrame" inherits="FUnitFrameTemplate, SecureUnitButtonTemplate">
    <Anchors>
      <Anchor point="CENTER" relativeTo="UIParent" relativePoint="CENTER" />
    </Anchors>
    <KeyValues>
      <KeyValue key="unit" value="player" type="string" />
    </KeyValues>
  </Button>

  <Button name="FTargetFrame" inherits="FUnitFrameTemplate, SecureUnitButtonTemplate">
    <Anchors>
      <Anchor point="CENTER" relativeTo="UIParent" relativePoint="CENTER" x="300" />
    </Anchors>
    <KeyValues>
      <KeyValue key="unit" value="target" type="string" />
    </KeyValues>
  </Button>
</Ui>